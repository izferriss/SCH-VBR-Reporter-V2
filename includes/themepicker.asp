<div class="themepickercontainer">
    <form>
        <label class="picker" for="themes">Theme:</label>
        <select id="themepicker" name="themes" id="cars" onchange="switchTheme(this)">
            <option value="default">Default</option>
            <option value="green">Green</option>
            <option value="dark">Dark</option>
            <option value="sch">SCH</option>
            <option value="lcars">LCARS</option>
        </select>
    </form>
</div>